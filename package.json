{
  "name": "od-labs",
  "version": "1.0.0",
  "description": "OD Labs Payload CMS Website",
  "license": "MIT",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "*.less",
    "./src/app/globals.css",
    "./src/utilities/accessibility.css",
    "./src/utilities/animations.css"
  ],
  "scripts": {
    "build": "pnpm run validate:identifiers && cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run type-check && pnpm run lint && pnpm run format:check",
    "validate:identifiers": "tsx scripts/validate-identifiers.js",
    "db:reset": "tsx scripts/reset-database-schema.js",
    "validate:identifiers:verbose": "tsx scripts/validate-identifiers.js --verbose",
    "validate:identifiers:strict": "tsx scripts/validate-identifiers.js --fail-on-warnings",
    "analyze:database-schema": "tsx scripts/analyze-database-schema.ts",
    "inspect:database-tables": "tsx scripts/inspect-database-tables.js",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "test:watch": "cross-env NODE_OPTIONS=--no-deprecation vitest --config ./vitest.config.mts",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules dist",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {}\"",
    "analyze": "cross-env ANALYZE=true next build",
    "bundle-analyzer": "npx @next/bundle-analyzer"
  },
  "dependencies": {
    "@jest/globals": "^30.2.0",
    "@payloadcms/admin-bar": "^3.73.0",
    "@payloadcms/db-postgres": "^3.73.0",
    "@payloadcms/live-preview-react": "^3.73.0",
    "@payloadcms/next": "^3.73.0",
    "@payloadcms/plugin-form-builder": "^3.73.0",
    "@payloadcms/plugin-search": "^3.73.0",
    "@payloadcms/plugin-seo": "^3.73.0",
    "@payloadcms/richtext-lexical": "^3.73.0",
    "@payloadcms/translations": "^3.73.0",
    "@payloadcms/ui": "^3.73.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@types/prismjs": "^1.26.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "geist": "^1.5.1",
    "glob": "^13.0.0",
    "graphql": "^16.12.0",
    "isomorphic-dompurify": "^2.35.0",
    "lucide-react": "^0.378.0",
    "next": "15.4.10",
    "next-sitemap": "^4.2.3",
    "payload": "^3.73.0",
    "payload-dashboard-analytics": "^2.2.0",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "7.45.4",
    "sharp": "0.34.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/bundle-analyzer": "^15.5.10",
    "@playwright/test": "1.56.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/escape-html": "^1.0.4",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "22.19.3",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.23",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.4.7",
    "fast-check": "^4.5.3",
    "jest-axe": "^10.0.0",
    "jsdom": "26.1.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
